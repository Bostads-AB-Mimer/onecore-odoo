# Example env file for Odoo local development

# Odoo paths
ODOO_PATH=/path/to/odoo-installation
ODOO_ONECORE_PATH=/path/to/onecore-odoo

# Database configuration
DB_NAME=odoo
DB_USER=your_user
DB_HOST=localhost
DB_PORT=5432

# Runtime settings
ENV=local
DEV_MODE=xml

# Default modules
DEFAULT_MODULES="onecore_maintenance_extension,onecore_mail_extension,onecore_auth,onecore_ui,onecore_web_extension"